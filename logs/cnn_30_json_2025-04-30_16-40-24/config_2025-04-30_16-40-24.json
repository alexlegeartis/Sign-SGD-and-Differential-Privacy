{
    "code_version": "cnn_30_json",
    "dataset": "mnist",
    "num_workers": 2,
    "batch_size": 2000,
    "iterations_per_epoch": 100,
    "alpha_min": 2,
    "alpha_max": 20,
    "sigma_min": 0.1,
    "sigma_max": 2,
    "model_type": "optimal",
    "algorithms": [
        {
            "type": "signsgd",
            "enabled": false,
            "num_epochs": 30,
            "lr_scheduler": "lambda t: 0.01 / (t + 1)**(1/3)",
            "poisson_qn": 30,
            "label": "SignSGD (LR=0.01)"
        },
        {
            "type": "dpsignsgd",
            "enabled": true,
            "num_epochs": 20,
            "lr_scheduler": "lambda t: min(1, (t+1)/1000) * 0.0005 / (t+1)**(0.08)",
            "poisson_qn": 10,
            "repeat_num": 1,
            "clipping_level": "lambda t: 4",
            "final_epsilon": 10.0,
            "final_power_in_delta": 1.1,
            "label": "DPSignSGD (LR~1/t^0.0.08)"
        },
        {
            "type": "dpsignsgd",
            "enabled": true,
            "num_epochs": 100,
            "lr_scheduler": "lambda t: min(1, (t+1)/1000) * 0.0005",
            "poisson_qn": 10,
            "repeat_num": 1,
            "clipping_level": "lambda t: 4",
            "final_epsilon": 10.0,
            "final_power_in_delta": 1.1,
            "label": "DPSignSGD (const LR)"
        },
        {
            "type": "dpsgd",
            "enabled": false,
            "num_epochs": 30,
            "lr_scheduler": "lambda t: 0.01 / (t + 1)**(1/3)",
            "poisson_qn": 200,
            "clipping_level": "lambda t: 4",
            "final_epsilon": 10.0,
            "final_power_in_delta": 1.1,
            "label": "DPSGD (LR=0.01)"
        },
        {
            "type": "fedsgd",
            "enabled": false,
            "num_epochs": 30,
            "lr_scheduler": "lambda t: 0.01 / (t + 1)**(1/3)",
            "poisson_qn": 100,
            "label": "FedSGD (LR=0.01)"
        }
    ]
} 